{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "ApiBaseUrl": {
        "value": "${AZURE_API_BASE_URL}"
      },
      "ApiPassword": {
        "value": "${AZURE_API_PASSWORD}"
      },
      "ApiUsername": {
        "value": "${AZURE_API_USERNAME}"
      },
      "Auth0Audience": {
        "value": "${AZURE_AUTH0_AUDIENCE}"
      },
      "Auth0ClientId": {
        "value": "${AZURE_AUTH0_CLIENT_ID}"
      },
      "Auth0Domain": {
        "value": "${AZURE_AUTH0_DOMAIN}"
      },
      "postgres_password": {
        "value": "${AZURE_POSTGRES_PASSWORD}"
      },
      "postgres_username": {
        "value": "${AZURE_POSTGRES_USERNAME}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      }
    }
  }
  